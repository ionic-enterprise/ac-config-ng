{
  "name": "ac-config-ng",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f4f5f8' --splashBackgroundColor '#f4f5f8' --iconBackgroundColorDark '#222428' --splashBackgroundColorDark '#222428'",
    "build": "npm run clean && npm run config && ng build && cap sync",
    "clean": "git checkout android/variables.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "config": "(export $(cat .env | xargs); npm run clean; node scripts/buildConfig.js ; npm run trapeze)",
    "lint": "ng lint",
    "ng": "ng",
    "prepare": "husky",
    "start": "ng serve --port=8100",
    "test": "ng test --no-watch",
    "test:dev": "ng test",
    "trapeze": "trapeze run -y config.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.0.4",
    "@angular/core": "^19.0.4",
    "@angular/forms": "^19.0.4",
    "@angular/platform-browser": "^19.0.4",
    "@angular/platform-browser-dynamic": "^19.0.4",
    "@angular/router": "^19.0.4",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic-enterprise/auth": "^6.0.4",
    "@ionic/angular": "^8.4.1",
    "ionicons": "~7.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular/cli": "^19.0.5",
    "@angular/compiler": "^19.0.4",
    "@angular/compiler-cli": "^19.0.4",
    "@angular/language-service": "^19.0.4",
    "@capacitor/assets": "~3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@ionic/angular-toolkit": "^12.1.1",
    "@ionic/prettier-config": "~4.0.0",
    "@trapezedev/configure": "^7.1.3",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^22.10.1",
    "angular-eslint": "^19.0.2",
    "eslint": "^9.17.0",
    "husky": "~9.1.5",
    "jasmine-core": "^5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "ts-node": "~10.9.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.1"
  },
  "description": "An Ionic project"
}
